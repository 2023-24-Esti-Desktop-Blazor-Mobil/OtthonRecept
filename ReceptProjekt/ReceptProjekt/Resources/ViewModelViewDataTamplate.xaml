<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    
                    xmlns:mainViewModel="clr-namespace:ReceptProjekt.ViewModels"
                    xmlns:mainView="clr-namespace:ReceptProjekt.Views"
                               >
  
    <DataTemplate DataType="{x:Type mainViewModel:MainViewModel}">
        <mainView:MainWindow/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type mainViewModel:CikkViewModel}">
        <mainView:CikkView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type mainViewModel:SzemelyViewModel}">
        <mainView:SzemelyView/>
    </DataTemplate>

</ResourceDictionary>