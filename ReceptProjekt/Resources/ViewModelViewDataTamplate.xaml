<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mainView="clr-namespace:ReceptProjekt.Views"
    xmlns:mainViewModel="clr-namespace:ReceptProjekt.ViewModels">

    <DataTemplate DataType="{x:Type mainViewModel:MainViewModel}">
        <mainView:MainWindow />
    </DataTemplate>
    <DataTemplate DataType="{x:Type mainViewModel:CikkViewModel}">
        <mainView:CikkView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type mainViewModel:SzemelyViewModel}">
        <mainView:SzemelyView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type mainViewModel:BelepesViewModel}">
        <mainView:Belepes />
    </DataTemplate>
    <DataTemplate DataType="{x:Type mainViewModel:ReceptViewModel}">
        <mainView:ReceptView />
    </DataTemplate>

</ResourceDictionary>